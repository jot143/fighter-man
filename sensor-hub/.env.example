# BLE Device MAC Addresses
# Copy this file to .env and update with your actual device MAC addresses

# Left foot pressure sensor
LEFT_FOOT_MAC=ed:63:5b:c4:2d:92

# Right foot pressure sensor (scan to find this)
RIGHT_FOOT_MAC=XX:XX:XX:XX:XX:XX

# Accelerometer IMU sensor (WT901BLE67)
ACCELEROMETER_MAC=c7:f7:92:82:f2:f9

# Performance Tuning
# Throttle: Process every Nth packet (1=no throttle, 2=half rate, 5=one-fifth rate)
FOOT_THROTTLE=2              # Foot sensor throttle (default: 2, ~10Hz from 20Hz)
ACCEL_THROTTLE=5             # Accelerometer throttle (default: 5, ~20Hz from 100Hz)

# Connection retry attempts (1-10)
CONNECTION_RETRIES=3         # Max connection attempts per device (default: 3)
